{% extends 'base.html.twig' %}

{% block title %}Carburant{% endblock %}

{% block body %}
    <h1>Carburant</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ carburant.id }}</td>
            </tr>
            <tr>
                <th>Type de carburant</th>
                <td>{{ carburant.nomCarburant }}</td>
            </tr>
        </tbody>
    </table>

    <a class="btn btn-primary" href="{{ path('carburant_index') }}">Retour</a>

    <a class="btn btn-primary" href="{{ path('carburant_edit', {'id': carburant.id}) }}">Modifier</a>

{% endblock %}
