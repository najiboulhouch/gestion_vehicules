<form method="post" action="{{ path('voiture_delete', {'id': voiture.id}) }}" 
onsubmit="return confirm('Voulez-vous supprimer cette voiture?');" style="display:inline;">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ voiture.id) }}">
    <button class="btn btn-danger"><i class="bi bi-trash"></i></button>
</form>
